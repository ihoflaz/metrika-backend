{
    "name": "metrika-backend",
    "version": "1.0.0",
    "description": "Metrika platformu icin backend uygulamasi",
    "private": true,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "directories": {
        "doc": "docs"
    },
    "engines": {
        "node": ">=22 <23"
    },
    "scripts": {
        "build": "tsc --pretty",
        "clean": "rimraf dist",
        "dev": "ts-node --transpile-only src/server.ts",
        "start": "node dist/server.js",
        "lint": "eslint --max-warnings=0 --ext .ts src tests",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write \"{src,tests}/**/*.ts\"",
        "format:check": "prettier --check \"{src,tests}/**/*.ts\"",
        "test": "cross-env NODE_ENV=test jest --runInBand --forceExit --detectOpenHandles",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:deploy": "prisma migrate deploy",
        "db:seed": "prisma db seed"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/express-serve-static-core": "^5.1.0",
        "@types/jest": "^29.5.14",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.9.2",
        "@types/node-cron": "^3.0.11",
        "@types/nodemailer": "^7.0.3",
        "@types/supertest": "^6.0.3",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "artillery": "^2.0.26",
        "cross-env": "^10.1.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "jest": "^29.7.0",
        "pino-pretty": "^13.1.2",
        "prettier": "^3.6.2",
        "rimraf": "^6.1.0",
        "supertest": "^7.1.4",
        "testcontainers": "^11.7.2",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.669.0",
        "@prisma/client": "^6.18.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/pdfkit": "^0.17.3",
        "argon2": "^0.44.0",
        "awilix": "^12.0.5",
        "bullmq": "^5.63.0",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv-flow": "^4.1.0",
        "exceljs": "^4.4.0",
        "express": "^5.1.0",
        "handlebars": "^4.7.8",
        "jsonwebtoken": "^9.0.2",
        "lru-cache": "^11.2.2",
        "multer": "^1.4.5-lts.2",
        "node-cron": "^3.0.3",
        "nodemailer": "^6.10.1",
        "pdfkit": "^0.17.2",
        "pino": "^10.1.0",
        "prisma": "^6.18.0",
        "puppeteer": "^21.11.0",
        "uuidv7": "^1.0.2",
        "winston": "^3.18.3",
        "winston-slack-webhook-transport": "^2.3.6",
        "zod": "^4.1.12"
    },
    "prisma": {
        "seed": "ts-node --transpile-only prisma/seed.ts"
    }
}
