{
    "name":  "metrika-backend",
    "version":  "1.0.0",
    "description":  "Metrika platformu icin backend uygulamasi",
    "private":  true,
    "main":  "dist/index.js",
    "types":  "dist/index.d.ts",
    "directories":  {
                        "doc":  "docs"
                    },
    "engines":  {
                    "node":  "\u003e=22 \u003c23"
                },
    "scripts":  {
                    "build":  "tsc --pretty",
                    "clean":  "rimraf dist",
                    "dev":  "ts-node --transpile-only src/server.ts",
                    "start":  "node dist/server.js",
                    "lint":  "eslint --max-warnings=0 --ext .ts src tests",
                    "lint:fix":  "npm run lint -- --fix",
                    "format":  "prettier --write \"{src,tests}/**/*.ts\"",
                    "format:check":  "prettier --check \"{src,tests}/**/*.ts\"",
                    "test":  "cross-env NODE_ENV=test jest --runInBand",
                    "prisma:generate":  "prisma generate",
                    "prisma:migrate":  "prisma migrate dev",
                    "prisma:deploy":  "prisma migrate deploy",
                    "db:seed":  "prisma db seed"
                },
    "keywords":  [

                 ],
    "author":  "",
    "license":  "MIT",
    "devDependencies":  {
                            "@types/express":  "^5.0.5",
                            "@types/express-serve-static-core":  "^5.1.0",
                            "@types/jest":  "^29.5.14",
                            "@types/node":  "^24.9.2",
                            "@types/supertest":  "^6.0.3",
                            "@typescript-eslint/eslint-plugin":  "^8.46.2",
                            "@typescript-eslint/parser":  "^8.46.2",
                            "cross-env":  "^10.1.0",
                            "eslint":  "^8.57.1",
                            "eslint-config-airbnb-base":  "^15.0.0",
                            "eslint-config-prettier":  "^10.1.8",
                            "eslint-import-resolver-typescript":  "^4.4.4",
                            "eslint-plugin-import":  "^2.32.0",
                            "eslint-plugin-prettier":  "^5.5.4",
                            "jest":  "^29.7.0",
                            "pino-pretty":  "^13.1.2",
                            "prettier":  "^3.6.2",
                            "rimraf":  "^6.1.0",
                            "supertest":  "^7.1.4",
                            "testcontainers":  "^11.7.2",
                            "ts-jest":  "^29.4.5",
                            "ts-node":  "^10.9.2",
                            "typescript":  "^5.9.3"
                        },
    "dependencies":  {
                         "@prisma/client":  "^6.18.0",
                         "argon2":  "^0.44.0",
                         "awilix":  "^12.0.5",
                         "dotenv-flow":  "^4.1.0",
                         "express":  "^5.1.0",
                         "jose":  "^6.1.0",
                         "pino":  "^10.1.0",
                         "prisma":  "^6.18.0",
                         "uuidv7":  "^1.0.2",
                         "zod":  "^4.1.12"
                     },
    "prisma":  {
                   "seed":  "ts-node --transpile-only prisma/seed.ts"
               }
}
